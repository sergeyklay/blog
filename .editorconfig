# top-most EditorConfig file
root                     = true

[*]
end_of_line              = lf
insert_final_newline     = true
indent_style             = space
charset                  = utf-8
indent_size              = 4
trim_trailing_whitespace = true

[*.{html,xml,css,yml,js,scss,md,json,svg}]
indent_size              = 2

[*.{svg}]
insert_final_newline     = false
